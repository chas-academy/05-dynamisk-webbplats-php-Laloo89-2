{
  "": {
    "controller": "Default",
    "method": "start"
  },
  "login": {
     "controller": "Admin",
     "method": "login"
  },
  "register": {
    "controller": "Admin",
    "method": "register"
  },
  "posts/:page": {
    "controller": "Post",
    "method": "getAllWithPage",
    "params": {
      "page": "number"
    }
  },
  "posts": {
    "controller": "Post",
    "method": "getAll"
  },
  "post/:id": {
    "controller": "Post",
    "method": "get",
    "params": {
      "id": "text"
    }
  },
  "posts/search": {
    "controller": "Post",
    "method": "search"
  },
  "admin": {
    "controller": "Admin",
    "method": "getAll",
    "login": true
  },
  "admin/:id": {
    "controller": "Admin",
    "method": "get",
    "params": {
      "id": "number",
      "login": true
    }
  },
  "myposts": {
    "controller": "Post",
    "method": "getByPost",
    "login": true
  }
}
